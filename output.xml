                  <ns0:value type="text">Reappointment Charges</ns0:value>
               </ns0:operand>
            </ns0:term>
         </ns0:expression>
      </ns0:rule>
      <ns0:rule id="15372" name="Check_SWIFT___ABS_Slotting_">
         <ns0:description>Check SWIFT / ABS Slotting?</ns0:description>
         <ns0:expression xsi:type="structuredExpressionType">
            <ns0:term>
               <ns0:operand xsi:type="orderDataOperandType">
                  <ns0:orderData>/order_details/swift_flag</ns0:orderData>
               </ns0:operand>
               <ns0:equals />
               <ns0:operand xsi:type="literalOperandType">
                  <ns0:value type="text">Y</ns0:value>
               </ns0:operand>
            </ns0:term>
         </ns0:expression>
      </ns0:rule>
      <ns0:rule id="15421" name="Fail_due_to__Customer__">
         <ns0:description>Fail due to 'Customer'?</ns0:description>
         <ns0:expression xsi:type="structuredExpressionType">
            <ns0:term>
               <ns0:operand xsi:type="orderDataOperandType">
                  <ns0:orderData>/prov/service/installation_error_message</ns0:orderData>
               </ns0:operand>
               <ns0:equals />
               <ns0:operand xsi:type="literalOperandType">
                  <ns0:value type="text">Due to Customer</ns0:value>
               </ns0:operand>
            </ns0:term>
         </ns0:expression>
      </ns0:rule>
      <ns0:rule id="15447" name="R3_Check_Fix_Flag_">
